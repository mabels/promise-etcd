{
  "name": "promise-etcd",
  "version": "1.0.4",
  "description": "promise based etcd api includes master election for workers",
  "main": "./promise-etcd.js",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "webpack",
    "build:watch": "webpack --watch --debug --output-pathinfo --progress",
    "pretest": "npm run build",
    "test": "mocha --require ./dist/etcd_daemon.js dist/test",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w",
    "preversion": "npm test",
    "version": "npm run build && conventional-changelog -p angular -v -r 0 > CHANGELOG.md && git add -A .",
    "postversion": "git push && git push --tags",
    "prepublish": "not-in-install && npm run build || echo 'In install cycle, skipping prepublish'"
  },
  "repository": {
    "type": "git",
    "url": "mabels/promise-etcd"
  },
  "license": "AFL-2.0",
  "bugs": {
    "url": "https://github.com/mabels/promise-etcd/issues"
  },
  "homepage": "https://github.com/mabels/promise-etcd#readme",
  "devDependencies": {
    "@types/chai": ">=3.4.33",
    "@types/mocha": ">=2.2.33",
    "chai": "3.5.0",
    "cz-conventional-changelog": ">=1.1.6",
    "globby": ">=5.0.0",
    "mocha": ">=3.2.0",
    "rimraf": ">=2.5.2",
    "shelljs": ">=0.7.6",
    "source-map-support": ">=0.4.11",
    "ts-loader": ">=1.3.3",
    "tslint": ">=3.11.0",
    "typescript": ">=2.1.1",
    "webpack": ">=1.14.0"
  },
  "dependencies": {
    "@types/node-uuid": ">=0.0.28",
    "@types/request-promise": ">=4.1.33",
    "node-uuid": ">=1.4.7",
    "request-promise": ">=4.1.1",
    "request": ">=2.60"
  },
  "publishConfig": {
    "tag": "next"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
