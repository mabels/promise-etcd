{
  "name": "promise-etcd",
  "version": "1.1.0",
  "description": "promise based etcd api includes master election for workers",
  "main": "./dist/lib/index.js",
  "types": "./dist/lib/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsc",
    "lint": "tslint --project tsconfig.json",
    "pretest": "npm run build",
    "test": "mocha --require ./dist/test/etcd_daemon.js dist/test/*-test.js",
    "prepublish": "npm run lint && npm run clean && npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "mabels/promise-etcd"
  },
  "license": "AFL-2.0",
  "bugs": {
    "url": "https://github.com/mabels/promise-etcd/issues"
  },
  "homepage": "https://github.com/mabels/promise-etcd#readme",
  "devDependencies": {
    "@types/chai": ">=3.4.33",
    "@types/mocha": "^2.2.43",
    "chai": ">=3.5.0",
    "cz-conventional-changelog": ">=1.1.6",
    "mocha": "^3.5.3",
    "rimraf": "^2.6.2",
    "tslint": ">=3.11.0",
    "typescript": ">=2.4.1"
  },
  "dependencies": {
    "@types/node-uuid": ">=0.0.28",
    "@types/winston": "^2.3.5",
    "@types/yargs": "^8.0.2",
    "@types/request": "^2.0.3",
    "node-uuid": ">=1.4.7",
    "winston": "^2.3.1",
    "yargs": "^8.0.2",
    "request": ">=2.60"
  },
  "publishConfig": {
    "tag": "next"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
